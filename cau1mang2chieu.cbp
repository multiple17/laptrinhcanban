#include<stdio.h>
void in_mt(int a[][3],int n,int m){
    for (int i=0; i<n; i++) {
        for (int j =0; j<m; j++){
            printf("%d\t",a[i][j]);
        }
        printf("\n");
    }

}
void nhap_mt(int *a,int n,int m){
    printf("nhap ma tran:\n");
    for (int i =0; i<n; i++){
        for (int j = 0; j<m; j++){
            scanf("%d",a[i][j]);
        }
    }
}
int main() {
    int a[3][3] = {{1,2,3},{2,3,4},{3,4,5}},b[3][3],*p,*q;
    p =(int*)a;
    q =(int*)b;
    in_mt(a,3,3);
    nhap_mt(b,3,3);
    in_mt(b,3,3);

}
